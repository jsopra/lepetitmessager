{
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:perspectivain/yii2-phactory.git"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "ext-redis": "*",
        "yiisoft/yii2": "*",
        "vlucas/phpdotenv": "2.*",
        "yiisoft/yii2-redis": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "perspectiva/yii2-phactory": "*"
    },
    "suggest": {

    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "writable": [
            "runtime",
            "web/assets"
        ],
        "executable": [
            "yii"
        ]
    }
}
